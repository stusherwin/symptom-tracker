    "elm-hot-webpack-loader": "^1.1.7",
    "elm-webpack-loader": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jquery": "^3.2.1",
    "mini-css-extract-plugin": "^1.3.3",
